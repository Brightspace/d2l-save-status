{
  "name": "d2l-save-status",
  "version": "1.6.0",
  "description": "Polymer Web-Component to display saving statuses",
  "private": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "polymer install --variants",
    "lint": "npm run lint:wc && npm run lint:html",
    "lint:html": "eslint *.html demo/*.html test/*.html",
    "lint:wc": "polymer lint",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local",
    "test": "npm run lint && npm run test:polymer:local"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/save-status.git"
  },
  "keywords": [
    "d2l",
    "save",
    "status",
    "statuses"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-brightspace": "^0.4.1",
    "eslint-plugin-html": "^5.0.0",
    "frau-ci": "^1.33.2",
    "polymer-cli": "^1.9.1",
    "web-component-tester-istanbul": "^0.10.0"
  }
}
